{
  "name": "@grogqli/webapp",
  "version": "0.4.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start:dev": "tsdx watch --verbose --onSuccess \"touch ./dist/success.txt\"",
    "build:publish": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "lint:output": "tsdx lint --write-file",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "__start": "tsdx watch",
    "__build": "tsdx build",
    "__test": "tsdx test --passWithNoTests",
    "__lint": "tsdx lint",
    "__prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Dean Slama",
  "module": "dist/webapp.esm.js",
  "size-limit": [
    {
      "path": "dist/webapp.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/webapp.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@apollo/client": "^3.3.7",
    "@babel/core": "^7.12.10",
    "@grogqli/schema": "^0.4.0",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/addons": "^6.1.15",
    "@storybook/react": "^6.1.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
    "babel-loader": "^8.2.2",
    "graphql": "^15.4.0",
    "husky": "^4.3.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "size-limit": "^4.9.1",
    "styled-components": "^5.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@apollo/client": "^3.3.7",
    "@grogqli/schema": "^0.4.0",
    "graphql": "^15.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0"
  },
  "dependencies": {
    "@rooks/use-key": "^3.6.0",
    "final-form": "^4.20.1",
    "graphql-formatter": "^1.0.3",
    "react-final-form": "^6.5.2",
    "react-icons": "^3.11.0"
  }
}
