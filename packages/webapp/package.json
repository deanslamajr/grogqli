{
  "name": "@grogqli/webapp",
  "version": "0.10.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --verbose",
    "start:dev": "tsdx watch --verbose --onSuccess \"touch ./dist/success.txt\"",
    "build:publish": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "lint:output": "tsdx lint --write-file",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Dean Slama",
  "module": "dist/webapp.esm.js",
  "size-limit": [
    {
      "path": "dist/webapp.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/webapp.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@grogqli/schema": "^0.10.0",
    "@rooks/use-key": "^3.6.0",
    "final-form": "^4.20.1",
    "graphql-formatter": "^1.0.3",
    "querystring": "^0.2.0",
    "react-final-form": "^6.5.2",
    "react-icons": "^3.11.0",
    "react-json-view": "^1.21.1"
  },
  "peerDependencies": {
    "@apollo/client": "^3.3.7",
    "graphql": "^15.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@grogqli/clients": "^0.10.0",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/addons": "^6.1.15",
    "@storybook/react": "^6.1.15",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql": "^15.4.0",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "tsdx": "^0.14.1",
    "typescript": "^4.1.3"
  }
}
