{
  "name": "nextjs-with-grogqli-example",
  "private": true,
  "version": "0.11.0",
  "author": "",
  "license": "MIT",
  "scripts": {
    "grogqli": "grogqli",
    "dev": "SCHEMA_PATH=base concurrently --raw npm:codegen:watch next",
    "dev:1": "SCHEMA_PATH=typeRemoved concurrently --raw npm:codegen:watch next",
    "dev:2": "SCHEMA_PATH=typeChangedKind concurrently --raw npm:codegen:watch next",
    "dev:3": "SCHEMA_PATH=fieldChangedKind concurrently --raw npm:codegen:watch next",
    "dev:4": "SCHEMA_PATH=fieldChangedType concurrently --raw npm:codegen:watch next",
    "dev:5": "SCHEMA_PATH=typeRemovedFromUnion concurrently --raw npm:codegen:watch next",
    "build": "npm run codegen && next build",
    "test": "npm run codegen && jest",
    "start": "next start",
    "lint": "eslint --fix --ext 'js,jsx,ts,tsx' .",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "npm run codegen -- -w"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@graphql-tools/load-files": "6.0.18",
    "@graphql-tools/merge": "6.0.18",
    "@graphql-tools/schema": "6.0.18",
    "apollo-server-micro": "^2.21.0",
    "faker": "^5.4.0",
    "graphql": "^15.4.0",
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.1",
    "@graphql-codegen/plugin-helpers": "^1.17.8",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@graphql-codegen/typescript-resolvers": "^1.17.8",
    "@grogqli/clients": "^0.11.0",
    "@grogqli/server": "^0.11.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "16.9.3",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-jest": "26.3.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql-tag": "2.11.0",
    "jest": "26.4.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^4.1.3",
    "yaml-loader": "0.6.0"
  }
}
