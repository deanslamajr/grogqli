{
  "name": "@grogqli/storybook",
  "version": "0.10.1",
  "description": "A storybook addon that consumes grogqli data",
  "license": "MIT",
  "main": "dist/preset.js",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "babel ./src --out-dir ./dist --watch --verbose --extensions '.js,.jsx,.ts,.tsx'",
    "check-types": "tsc --watch --preserveWatchOutput",
    "start:dev": "concurrently --raw npm:check-types npm:build",
    "build-storybook": "build-storybook -o ./docs-build -s ./public",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "start-storybook -p 6006 -s public --no-manager-cache"
  },
  "dependencies": {
    "msw": "^0.26.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.7",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.17",
    "@storybook/react": "^6.1.14",
    "auto": "^10.3.0",
    "babel-loader": "^8.1.0",
    "chromatic": "^5.6.1",
    "concurrently": "^5.3.0",
    "graphql": "^15.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@storybook/addons": "^6.1.14",
    "@storybook/api": "^6.1.14",
    "@storybook/components": "^6.1.14",
    "@storybook/core-events": "^6.1.14",
    "@storybook/theming": "^6.1.14",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "storybook": {
    "displayName": "Addon Kit",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
